					{
						"name": "Nettoinnflytting, inkl. inn- og utvandring",
						"type": "INT64"
					},
					{
						"name": "kvartal",
						"type": "UTF8"
					}
				]
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/mcrvpiraw')]"
			]
		},
		{
