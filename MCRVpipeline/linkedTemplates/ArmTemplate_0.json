{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "MCRVpipeline"
		},
		"ls_blob_eikon_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_blob_eikon'"
		},
		"ls_mcrvpip_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_mcrvpip'"
		},
		"ls_mcrvpip_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://mcrvpip.dfs.core.windows.net/"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/ls_blob_eikon')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobStorage",
				"typeProperties": {
					"connectionString": "[parameters('ls_blob_eikon_connectionString')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/ls_mcrvpip')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobFS",
				"typeProperties": {
					"url": "[parameters('ls_mcrvpip_properties_typeProperties_url')]",
					"accountKey": {
						"type": "SecureString",
						"value": "[parameters('ls_mcrvpip_accountKey')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/ds_SWAPS')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "ls_mcrvpip",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "DelimitedText",
				"typeProperties": {
					"location": {
						"type": "AzureBlobFSLocation",
						"fileName": "swaps.csv",
						"fileSystem": "raw"
					},
					"columnDelimiter": ",",
					"escapeChar": "\\",
					"firstRowAsHeader": true,
					"quoteChar": "\""
				},
				"schema": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/ls_mcrvpip')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/ds_eikon1_xlsx')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "ls_blob_eikon",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "Excel",
				"typeProperties": {
					"sheetName": "Sheet1",
					"location": {
						"type": "AzureBlobStorageLocation",
						"fileName": "eikon1.xlsx",
						"container": "eikon"
					},
					"firstRowAsHeader": true
				},
				"schema": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/ls_blob_eikon')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/pl_ingest_Eikon')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Copy Eikon Data",
						"type": "Copy",
						"dependsOn": [],
						"policy": {
							"timeout": "0.00:05:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"source": {
								"type": "ExcelSource",
								"storeSettings": {
									"type": "AzureBlobStorageReadSettings",
									"recursive": true,
									"enablePartitionDiscovery": false
								}
							},
							"sink": {
								"type": "DelimitedTextSink",
								"storeSettings": {
									"type": "AzureBlobFSWriteSettings"
								},
								"formatSettings": {
									"type": "DelimitedTextWriteSettings",
									"quoteAllText": true,
									"fileExtension": ".txt"
								}
							},
							"enableStaging": false,
							"translator": {
								"type": "TabularTranslator",
								"mappings": [
									{
										"source": {
											"name": "Dates",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "Dates",
											"type": "String",
											"physicalType": "String"
										}
									},
									{
										"source": {
											"name": "OINOK3MD=",
											"type": "Decimal",
											"physicalType": "String"
										},
										"sink": {
											"name": "OINOK3MD=",
											"type": "String",
											"physicalType": "String"
										}
									},
									{
										"source": {
											"name": "NOKAB6O5Y=",
											"type": "Decimal",
											"physicalType": "String"
										},
										"sink": {
											"name": "NOKAB6O5Y=",
											"type": "String",
											"physicalType": "String"
										}
									},
									{
										"source": {
											"name": "NOKAB6O10Y=",
											"type": "Decimal",
											"physicalType": "String"
										},
										"sink": {
											"name": "NOKAB6O10Y=",
											"type": "String",
											"physicalType": "String"
										}
									},
									{
										"source": {
											"name": "NOKAB6O5YF1Y=",
											"type": "Decimal",
											"physicalType": "String"
										},
										"sink": {
											"name": "NOKAB6O5YF1Y=",
											"type": "String",
											"physicalType": "String"
										}
									},
									{
										"source": {
											"name": "NOKAB6O5YF2Y=",
											"type": "Decimal",
											"physicalType": "String"
										},
										"sink": {
											"name": "NOKAB6O5YF2Y=",
											"type": "String",
											"physicalType": "String"
										}
									},
									{
										"source": {
											"name": "NOKAB6O5YF3Y=",
											"type": "Decimal",
											"physicalType": "String"
										},
										"sink": {
											"name": "NOKAB6O5YF3Y=",
											"type": "String",
											"physicalType": "String"
										}
									},
									{
										"source": {
											"name": "NOKAB6O5YF5Y=",
											"type": "Decimal",
											"physicalType": "String"
										},
										"sink": {
											"name": "NOKAB6O5YF5Y=",
											"type": "String",
											"physicalType": "String"
										}
									}
								],
								"typeConversion": true,
								"typeConversionSettings": {
									"allowDataTruncation": true,
									"treatBooleanAsNumber": false
								}
							}
						},
						"inputs": [
							{
								"referenceName": "ds_eikon1_xlsx",
								"type": "DatasetReference",
								"parameters": {}
							}
						],
						"outputs": [
							{
								"referenceName": "ds_SWAPS",
								"type": "DatasetReference",
								"parameters": {}
							}
						]
					}
				],
				"policy": {
					"elapsedTimeMetric": {},
					"cancelAfter": {}
				},
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/ds_eikon1_xlsx')]",
				"[concat(variables('factoryId'), '/datasets/ds_SWAPS')]"
			]
		}
	]
}