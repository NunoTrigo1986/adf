{
	"name": "df_append_eikon",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"name": "newparquet"
				},
				{
					"dataset": {
						"referenceName": "ds_original",
						"type": "DatasetReference"
					},
					"name": "existingparquet"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_original",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "signaturenew",
					"description": "__index_level_0__, fingerprint'"
				},
				{
					"name": "signatureexisting",
					"description": "__"
				},
				{
					"name": "exists1"
				},
				{
					"name": "samle"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Date as timestamp,",
				"          Policy_rate_daily as double,",
				"          country_key as long,",
				"          indicator_key as long,",
				"          periodicity_key as long,",
				"          source_key as long,",
				"          unit_key as long",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false) ~> newparquet",
				"source(output(",
				"          Date as timestamp,",
				"          Policy_rate_daily as double,",
				"          country_key as long,",
				"          indicator_key as long,",
				"          periodicity_key as long,",
				"          source_key as long,",
				"          unit_key as long,",
				"          fingerprint as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'parquet') ~> existingparquet",
				"newparquet derive(fingerprint = sha2(256,columns())) ~> signaturenew",
				"existingparquet derive(fingerprint = sha2(256,columns())) ~> signatureexisting",
				"signaturenew, signatureexisting exists(signaturenew@fingerprint == signatureexisting@fingerprint,",
				"     negate:true,",
				"     broadcast: 'auto')~> exists1",
				"exists1, existingparquet union(byName: true)~> samle",
				"samle sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          Date as timestamp,",
				"          Policy_rate_daily as double,",
				"          country_key as long,",
				"          indicator_key as long,",
				"          periodicity_key as long,",
				"          source_key as long,",
				"          unit_key as long,",
				"          fingerprint as string",
				"     ),",
				"     format: 'parquet',",
				"     partitionFileNames:['CPI_mon_fact.parquet'],",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     saveOrder: 1,",
				"     partitionBy('hash', 1)) ~> sink1"
			]
		}
	}
}