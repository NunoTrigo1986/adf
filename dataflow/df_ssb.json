{
	"name": "df_ssb",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_ssb",
						"type": "DatasetReference"
					},
					"name": "ssbSource"
				}
			],
			"sinks": [
				{
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "columnHeaders",
					"description": "delete spaces on column headers"
				}
			],
			"scriptLines": [
				"source(output(",
				"          {FÃ¸dselsoverskot} as long,",
				"          {Nettoinnflytting, inkl. inn- og utvandring} as long,",
				"          kvartal as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'parquet') ~> ssbSource",
				"ssbSource select(mapColumn(",
				"          each(match(true()),",
				"               regexReplace($$,'[^a-zA-Z]','_') = $$)",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> columnHeaders",
				"columnHeaders sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink1"
			]
		}
	}
}